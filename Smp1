local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xzcReasd/scripts/refs/heads/main/Library"))()
local ThemeManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/xzcReasd/scripts/refs/heads/main/ThemeManager"))()
local SaveManager  = loadstring(game:HttpGet("https://raw.githubusercontent.com/xzcReasd/scripts/refs/heads/main/SaveManager"))()
ThemeManager:SetLibrary(Library)
SaveManager:SetLibrary(Library)
local Window = Library:CreateWindow({
    Name = "SimpleBot",
    Themeable = true,
    Info = "Generated with Linoria",
    Center = true,
    AutoShow = true
})

local Tabs = {
    Aimbot  = Window:AddTab('Aimbot'),
    Misc    = Window:AddTab('Misc'),
    Player  = Window:AddTab('Player'),
    Visuals = Window:AddTab('Visuals'),
    Config  = Window:AddTab('Config')
}
SaveManager:BuildConfigSection(Tabs.Config)
local ThemeGroupbox = Tabs.Config:AddLeftGroupbox('Theme Settings')
ThemeManager:ApplyToGroupbox(ThemeGroupbox)
ThemeManager:LoadDefault()
SaveManager:LoadAutoloadConfig()

